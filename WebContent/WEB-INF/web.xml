<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>scratchpad</display-name>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>user.userController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>dashboard</servlet-name>
  	<servlet-class>dashboard.dashboardController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>account</servlet-name>
  	<servlet-class>account.accountController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>save</servlet-name>
  	<servlet-class>save.saveController</servlet-class>
  </servlet>
  <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>account</servlet-name>
        <url-pattern>/account</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dashboard</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>save</servlet-name>
        <url-pattern>/test/index</url-pattern>
    </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>